@model Kiralama

<form method="post">
    <input asp-for="Id" hidden/>

    <div class="border p-3 mt-5">
        <div class="row pb-3">
            <h2 class="text-primary">Kiralama Kayit Sil</h2>

            <hr />

        </div>

       

        <div class="mb-3">
            <label asp-for="OgrenciId" class="p-0"></label>
            <input asp-for="OgrenciId" disabled class="form-control" />
          

        </div>
        
        <button type="submit" class="btn btn-lg btn-primary" style="width:250px">Kiralama Kayit Sil</button>

        <a asp-controller="Kiralama" asp-action="Index" class="btn btn-lg btn-primary" style="width:250px">Kiralama Listesine Geri Don</a>

    </div>
</form>